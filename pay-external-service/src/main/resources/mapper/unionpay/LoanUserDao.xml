<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.tfjt.pay.external.unionpay.dao.LoanUserDao">
    <select id="applicationStatusNotSucceededData" resultType="com.tfjt.pay.external.unionpay.entity.LoanUserEntity">
        SELECT
            *
        FROM
            tf_loan_user
        WHERE
            (application_status != 'succeeded' or application_status is null)
          AND out_request_no IS NOT NULL
        ORDER BY
            create_date DESC
    </select>

</mapper>
