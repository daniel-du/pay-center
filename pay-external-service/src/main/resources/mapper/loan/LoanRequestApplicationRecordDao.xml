<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.tfjt.pay.external.unionpay.dao.LoanRequestApplicationRecordDao">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.tfjt.pay.external.unionpay.entity.LoanRequestApplicationRecordEntity" id="loanRequestApplicationRecordMap">
        <result property="id" column="id"/>
        <result property="appId" column="app_id"/>
        <result property="requestUrl" column="request_url"/>
        <result property="requestParam" column="request_param"/>
        <result property="responseParam" column="response_param"/>
        <result property="responseTime" column="response_time"/>
        <result property="tradeOrderNo" column="trade_order_no"/>
        <result property="createTime" column="create_time"/>
    </resultMap>
    <select id="listError"
            resultType="com.tfjt.pay.external.unionpay.entity.LoanRequestApplicationRecordEntity">
        SELECT id,app_id,request_url,request_param,trade_order_no,trade_type FROM tf_loan_request_application_record r WHERE id in (
            select max(id) as id from tf_loan_request_application_record where create_time &gt;= #{date}
                group by app_id,trade_order_no having count(id) &lt;=10
            ) and callback_status=0

    </select>


</mapper>