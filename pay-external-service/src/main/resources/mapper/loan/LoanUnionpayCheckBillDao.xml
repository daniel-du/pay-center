<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.tfjt.pay.external.unionpay.dao.LoanUnionpayCheckBillDao">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.tfjt.pay.external.unionpay.entity.LoanUnionpayCheckBillEntity" id="loanUnionpayCheckBillMap">
        <result property="id" column="id"/>
        <result property="date" column="date"/>
        <result property="url" column="url"/>
        <result property="balanceAcctId" column="balance_acct_id"/>
        <result property="ceateTime" column="ceate_time"/>
    </resultMap>
    <select id="loadFile">
        LOAD DATA INFILE #{_parameter} ignore into table tf_loan_unionpay_check_bill_details
        fields terminated by ',' enclosed by ""
    </select>


</mapper>